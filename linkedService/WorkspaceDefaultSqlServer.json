{
	"name": "WorkspaceDefaultSqlServer",
	"properties": {
		"parameters": {
			"WorkspaceName": {
				"type": "string",
				"defaultValue": "Testing123"
			},
			"DBName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDW",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:@{linkedService().WorkspaceName}.sql.azuresynapse.net,1433;Initial Catalog=${linkedService().DBName}"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	}
}